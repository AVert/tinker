= doctype
%html{:lang=>'en'}
  %head
    %meta{:charset=>'utf-8'}
    - metas.each do |meta|
      = meta
    %title= title
    - stylesheets.each do |stylesheet|
      %link{:rel => 'stylesheet', :href => stylesheet}
    %style= css
    - scripts.each do |script|
      %script{:src => script}
  %body
    = html
    %script= js

